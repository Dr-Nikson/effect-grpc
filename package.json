  "devDependencies": {
    "@effect/eslint-plugin": "0.3.2",
    "@effect/language-service": "catalog:",
    "@eslint/js": "9.35.0",
    "@trivago/prettier-plugin-sort-imports": "6.0.0-0",
    "@tsconfig/node24": "24.0.1",
    "@types/node": "catalog:",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.6.2",
    "typescript-eslint": "8.43.0",
    "typescript": "catalog:",
    "tsx": "catalog:"
  },
  "dependencies": {
  },
  "engines": {
    "node": ">= 22"
  }
