{
  "name": "@dr_nikson/effect-grpc-example",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.16.1",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./protoc-gen-effect": {
      "import": {
        "types": "./dist/protocGenPlugin.d.ts",
        "default": "./dist/protocGenPlugin.js"
      },
      "require": {
        "types": "./dist/protocGenPlugin.d.ts",
        "default": "./dist/protocGenPlugin.js"
      }
    }
  },
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "clean": "rm -rf ./dist ./tsconfig.tsbuildinfo",
    "generate:proto": "pnpm buf generate",
    "build": "pnpm run clean && pnpm run generate:proto && pnpm run compile",
    "test": "echo \"No test specified\" && exit 0",
    "test:types": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "effect": "catalog:",
    "@bufbuild/protobuf": "catalog:",
    "@connectrpc/connect": "catalog:",
    "@dr_nikson/effect-grpc": "workspace:3.0.0-alpha.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "catalog:",
    "@bufbuild/protoc-gen-es": "catalog:",
    "@effect/platform": "catalog:",
    "@effect/platform-node": "catalog:",
    "effect": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}